<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,CT_CONSTRUCTOR_THROW,DP_CREATE_CLASSLOADER_INSIDE_DO_PRIVILEGED"/>
    </Match>
    <Match>
        <Class name="org.dellroad.javabox.JavaBox"/>
        <Method name="doExecute"/>
        <Bug pattern="SF_SWITCH_FALLTHROUGH"/>
    </Match>
    <Match>
        <Class name="org.dellroad.javabox.SnippetOutcomes$AbstractHasException"/>
        <Bug pattern="NM_CLASS_NOT_EXCEPTION"/>
    </Match>
</FindBugsFilter>
